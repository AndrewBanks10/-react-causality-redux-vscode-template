{
    "presets": [
        "env",
        "react"
    ],
    "plugins": [        
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        "react-hot-loader/babel",
        "transform-react-jsx",
        "babel-plugin-dynamic-import-node"
    ],
    "env": {
        "production": {
            "presets": [
                "react-optimize"
            ]
        },
        "development": {
            "plugins": [
                "transform-react-jsx-source"
            ]
        }
    },
    "sourceMaps": true,
    "retainLines": true
}